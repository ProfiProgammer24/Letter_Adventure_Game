<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="235.0" prefWidth="987.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.Letter_Adventure.control.Letter_Adventure_Congrats_Control">
   <stylesheets>
      <URL value="@startMenu.css" />
   </stylesheets>
   <children>
      <ImageView fitHeight="235.0" fitWidth="1006.0" layoutX="-4.0">
         <image>
            <Image url="@../../../bild/gold.jpg" />
         </image>
      </ImageView>
      <Label layoutX="74.0" layoutY="7.0" text="Gewonnen ! Du hast die maximale Punkteanzahl von 10 Punkten erreicht ">
         <font>
            <Font name="MV Boli" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btn_startNew" layoutX="15.0" layoutY="99.0" mnemonicParsing="false" onAction="#startNewGame" prefHeight="45.0" prefWidth="276.0" styleClass="buttonNewGame" text="Neu Starten">
         <font>
            <Font size="18.0" />
         </font>
      </Button>

      <Button layoutX="15.0" layoutY="167.0" mnemonicParsing="false" onAction="#endGame" prefHeight="45.0" prefWidth="276.0" styleClass="buttonEndGame" text="Spiel beenden">
         <font>
            <Font size="18.0" />
         </font>
      </Button>

      <Label layoutX="499.0" layoutY="57.0" text="--Dein Highscore--">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <ImageView fitHeight="190.0" fitWidth="147.0" layoutX="816.0" layoutY="43.0">
         <image>
            <Image url="@../../../bild/trophy.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
